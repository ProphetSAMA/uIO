<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="fun.wsss.uio.Mapper.RoomMapper">
    <select id="selectRoomsByParentId" resultType="fun.wsss.uio.Entity.room.Room">
        SELECT rooms_id, room_name, parent_id
        FROM rooms
        WHERE parent_id = #{parentId}
    </select>
</mapper>